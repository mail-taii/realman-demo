<launch>

    <arg name="rviz_gazebo_config" default="$(find embodied_moveit)/launch/moveit.rviz"/>
    <arg name="arm_type" default="rm65" />
    <arg name="end_effector" default="rmg24" />
    
    <include file="$(find embodied_moveit)/launch/move_group.launch">
        <arg name="publish_monitored_planning_scene" value="true" />
        <arg name="arm_type" value="$(arg arm_type)"/>       <!-- 新增 -->
        <arg name="end_effector" value="$(arg end_effector)"/><!-- 新增 -->
    </include>
    <include file="$(find embodied_moveit)/launch/moveit_rviz.launch">
        <arg name="rviz_config" value="$(arg rviz_gazebo_config)" />
    </include>

</launch>