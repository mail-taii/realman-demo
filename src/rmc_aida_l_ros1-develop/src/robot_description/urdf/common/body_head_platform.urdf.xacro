  <joint
    name="platform_joint"
    type="prismatic">
    <origin
      xyz="0 -0.11663 0.271"
      rpy="0 0 0" />
    <parent
      link="body_base_link" />
    <child
      link="platform_base_link" />
    <axis
      xyz="0 0 1" />
    <limit
      lower="0"
      upper="1"
      effort="60"
      velocity="0.15" />
  </joint>
  <joint
    name="head_joint1"
    type="revolute">
    <origin
      xyz="0 -0.1018 0.126"
      rpy="0 0 0" />
    <parent
      link="platform_base_link" />
    <child
      link="head_link1" />
    <axis
      xyz="0 0 1" />
    <limit
