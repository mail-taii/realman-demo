  <!-- Macro for visual and collision elements -->
  <xacro:macro name="visual_collision" params="mesh">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_path}${mesh}" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_path}${mesh}" />
      </geometry>
    </collision>
  </xacro:macro>

